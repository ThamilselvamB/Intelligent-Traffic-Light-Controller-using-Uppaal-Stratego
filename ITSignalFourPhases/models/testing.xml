<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
clock t;
broadcast chan C;</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.
typedef int[3,10] max_t;
int maxValue = 0;
void assignMax(int maxi){


    maxValue = maxi;
}</declaration>
		<location id="id0" x="0" y="0">
			<committed/>
		</location>
		<location id="id1" x="221" y="0">
			<label kind="invariant" x="211" y="17">t&lt;=5</label>
		</location>
		<location id="id2" x="110" y="-119">
			<committed/>
		</location>
		<init ref="id0"/>
		<transition controllable="false">
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="assignment" x="18" y="-59">t=0</label>
		</transition>
		<transition controllable="false">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="128" y="-93">t&gt;=5</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="18" y="-17">C!</label>
		</transition>
	</template>
	<template>
		<name>Template0</name>
		<declaration>// Place local declarations here.
typedef int[3,10] max_t;
int maxValue1[2] = {0,0};
void assignMax(int dir,int maxi){


    maxValue1[dir] = maxi;
}</declaration>
		<location id="id3" x="110" y="-119">
			<committed/>
		</location>
		<location id="id4" x="0" y="0">
		</location>
		<location id="id5" x="221" y="0">
			<committed/>
		</location>
		<location id="id6" x="-42" y="-127">
			<committed/>
		</location>
		<init ref="id4"/>
		<transition controllable="false">
			<source ref="id6"/>
			<target ref="id4"/>
		</transition>
		<transition controllable="false">
			<source ref="id3"/>
			<target ref="id6"/>
			<label kind="select" x="-24" y="-174">e:max_t</label>
			<label kind="assignment" x="-24" y="-123">assignMax(e)</label>
		</transition>
		<transition controllable="false">
			<source ref="id5"/>
			<target ref="id3"/>
			<label kind="select" x="128" y="-110">e:max_t</label>
			<label kind="assignment" x="128" y="-59">assignMax(e)</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="18" y="-17">C?</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
Process1 = Template0();
// List one or more processes to be composed into a system.
system Process,Process1;
    </system>
	<queries>
		<query>
			<formula>simulate 1[&lt;=500] { Process1.maxValue1 }</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
